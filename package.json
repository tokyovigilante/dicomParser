{
  "name": "dicom-parser",
  "version": "1.8.12",
  "description": "Javascript parser for DICOM Part 10 data",
  "main": "src/index.js",
  "module": "src/index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json"
  },
  "browser": {
    "zlib": false
  },
  "keywords": [
    "DICOM",
    "medical",
    "imaging"
  ],
  "author": "Chris Hafey",
  "homepage": "https://github.com/cornerstonejs/dicomParser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cornerstonejs/dicomParser/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cornerstonejs/dicomParser.git"
  },
  "scripts": {
    "build": "npm run version && npm run doc:generate",
    "clean": "npm run clean:coverage",
    "clean:coverage": "shx rm -rf coverage",
    "clean:docs": "shx rm -rf documentation",
    "doc": "npm run doc:generate && open-cli documentation/index.html",
    "doc:generate": "npm run clean:docs && jsdoc -c .jsdocrc",
    "eslint": "eslint -c .eslintrc.js src",
    "eslint-fix": "eslint -c .eslintrc.js --fix src",
    "eslint-quiet": "eslint -c .eslintrc.js --quiet src",
    "test": "mocha test/**/*_test.js",
    "test:node": "mocha test/**/*_test.js",
    "test:browser": "npm run test:chrome",
    "test:chrome": "karma start config/karma/karma-chrome.mjs",
    "test:firefox": "karma start config/karma/karma-firefox.js",
    "test:watch": "karma start config/karma/karma-watch.js",
    "version": "node -e \"import fs from 'fs'; import {readFileSync} from 'fs'; const pkg=JSON.parse(readFileSync('./package.json','utf8')); fs.writeFileSync('src/version.js', \\\"export default '\\\" + pkg.version + \\\"';\\n\\\");\""
  },
  "devDependencies": {
    "chai": "^5.1.0",
    "coveralls": "^3.1.1",
    "docdash": "^2.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.30.0",
    "jsdoc": "^4.0.3",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "lodash": "^4.17.21",
    "mocha": "^10.7.3",
    "open-cli": "^8.0.0",
    "puppeteer": "^23.3.0",
    "rusha": "^0.8.14",
    "shx": "^0.3.4"
  }
}
